{
    "name": "rickycezar/laravel-jwt-impersonate",
    "description": "Laravel Impersonate is a plugin that allows to you to authenticate as your users.",
    "type": "library",
    "keywords": [
      "laravel",
      "package",
      "plugin",
      "impersonate",
      "impersonation",
      "user",
      "auth",
      "laravel-package",
      "laravel-plugin"
    ],
    "require": {
        "php": "^8.0 | ^8.1",
        "laravel/framework": "^6.0 | ^7.0 | ^8.0 | ^9.0",
        "php-open-source-saver/jwt-auth": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 | ^8.0 | ^9.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^4.0 | ^5.0 | ^6.0 | ^7.0",
        "orchestra/database": "^4.0 | ^5.0 | ^6.0 | ^7.0"
    },
    "license": "MIT",
    "authors": [
      {
        "name": "rickycezar",
        "email": "barros.henrique.c@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "Rickycezar\\Impersonate\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Rickycezar\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
